{
  "coc.preferences.formatOnSaveFiletypes": ["dart"],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "python.venvFolders": [
    "~/.virtualenvs",
    ".virtualenvs"
  ],
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": false
}
